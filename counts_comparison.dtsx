                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--counts comparison
select 
a.table_category + '_' + convert(varchar(100), getdate() , 23) as version, a.*
from 
(
SELECT
case 
when t.name='/BIC/PZCUSTOMER' then N'customer'
when t.name='/BIC/AZSD_OD1900' then N'billing'
when t.name='/BIC/AZSD_OD1000' then N'delivery'
when t.name='/BIC/AZSD_OD0500' then N'sales_order'
end as table_category,
N'PBW' as stage_type,
t.name AS table_name,
sum(p.rows) as row_count,
getdate() as updated_time_edw
FROM sys.tables t
inner join sys.indexes i ON t.OBJECT_ID = i.object_id
inner join sys.partitions p ON i.object_id = p.OBJECT_ID AND i.index_id = p.index_id
inner join sys.allocation_units a ON p.partition_id = a.container_id
WHERE t.name NOT LIKE 'dt%'
AND i.OBJECT_ID &gt; 255
AND i.index_id &lt;= 1
and  t.name in 
('/BIC/AZSD_OD0500', 
'/BIC/AZSD_OD1000',
'/BIC/PZCUSTOMER' ,
'/BIC/AZSD_OD1900')
GROUP BY t.name, i.object_id, i.index_id, i.name
union all
select 'material' , 'PBW', '/BI0/MMATERIAL' , count(*), getdate() as updated_time_edw  from [pbw].[/BI0/MMATERIAL]
union all 
select 'customer_salesview', 'PBW', '/BI0/MCUST_SALES' , count(*), getdate() as updated_time_edw from [pbw].[/BI0/MCUST_SALES]
) a</property>
